config_version=5

[application]

config/name="Lost In Fear"
run/main_scene="res://scenes/world/outdoor.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.png"

[autoload]

Global="*res://autoloads/global.gd"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
AudioManager="*res://classes/audio_manager.tscn"

[dialogue_manager]

general/balloon_path="res://scenes/interfaces/balloon.tscn"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="lost-in-fear"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/better-terrain/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "green",
"res://dialogues/": "purple",
"res://resources/": "teal",
"res://scenes/": "yellow",
"res://scripts/": "red",
