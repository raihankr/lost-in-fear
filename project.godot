; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Lost In Fear"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.png"

[autoload]

BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
AudioManager="*res://autoloads/audio_manager.tscn"
SceneManager="*res://autoloads/scene_manager.gd"
SaveData="*res://autoloads/save_data.gd"
InGameUI="*res://autoloads/in_game_ui.tscn"
Global="*res://autoloads/global.gd"

[dialogue_manager]

general/balloon_path="res://scenes/interfaces/balloon.tscn"

[display]

window/stretch/mode="viewport"
window/size/always_on_top.debug=true

[dotnet]

project/assembly_name="lost-in-fear"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/better-terrain/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg")

[environment]

debug_mode=false
debug_mode.release=false

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "green",
"res://dialogues/": "purple",
"res://resources/": "teal",
"res://scenes/": "yellow",
"res://scripts/": "red",
"res://shaders/": "orange"
}

